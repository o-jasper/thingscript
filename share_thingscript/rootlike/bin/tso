# Puts the 'thing script objects' directory in the name.

DIRNAME=tso
if [ "$2" != "no" ]; then
   mkdir -p `dirname $1`/$DIRNAME/
fi
echo $el | grep -xP '.+\.stl' > /dev/null
if [ "$?" == "0" ]; then
    echo `dirname $1`/$DIRNAME/`basename $1`
else
    echo $1
fi
