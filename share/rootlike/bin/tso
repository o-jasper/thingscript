#!/bin/bash

echo $1 | grep '/tso/' > /dev/null
if [ "$?" == "0" ]; then
    ret=$(dirname $1)/$2$(basename $1)
    mkdir -p $(dirname $ret)
    echo $ret
    exit 0
fi

# Puts the 'thing script objects' directory in the name.
#(very thingscript specific thing to do)
ret=$(dirname $1)/tso/$2$(basename $1)
mkdir -p $(dirname $ret)
echo $ret
